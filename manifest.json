{
  "name": "meican chrome extension",
  "version": "1.0",
  "description": "定时自动点每餐",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/start.png",
      "32": "images/start.png",
      "48": "images/start.png",
      "128": "images/start.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "images/start.png",
    "32": "images/start.png",
    "48": "images/start.png",
    "128": "images/start.png"
  },
  "content_scripts": {
    
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "storage"
  ],
  "manifest_version": 2
}